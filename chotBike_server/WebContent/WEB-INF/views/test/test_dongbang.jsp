<script language="JavaScript">
var goods_link = new Array();
var goods_max = 0;
var goods_length = 4;
var goods_current = goods_max;


var divSave = null;

function todayview_visible()
{
    set_cookie('ck_tvhidden', '', 1);

    document.getElementById('divToday').innerHTML = divSave;
